{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "znodebasetest"
		},
		"AzureBlobStorageForADF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageForADF'"
		},
		"AzureSqlDatabaseForADF_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseForADF'"
		},
		"PipeLineTrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/d449b5fc-f6b7-4de6-8382-ef443f155907/resourceGroups/nonprod-znode-base/providers/Microsoft.Storage/storageAccounts/znodebase"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "znodebasetest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageForADF_connectionString": {
						"value": "[parameters('AzureBlobStorageForADF_connectionString')]"
					},
					"AzureSqlDatabaseForADF_connectionString": {
						"value": "[parameters('AzureSqlDatabaseForADF_connectionString')]"
					},
					"PipeLineTrigger_properties_typeProperties_scope": {
						"value": "[parameters('PipeLineTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}